{%- assign headerSnippetPath = 'header-password' -%}
{%- assign wfPage = '63b460d12385d14fe17dd609' -%}
{%- assign bodyClasses = '' -%}
<div data-w-id="0061e29d-1441-0c99-0a6c-4a01bfc02387" class="hero-section wf-section" style="padding-bottom : 0px;">
  {%- section 'navbar' -%}
</div>
<section class="section bg-transparent" style="padding-top : 3rem !important;">
  <div class="cart-heading">
    <h1 class="heading text-left">Cart Items</h1>
  </div>
  <div class="cart-items">
    <div class="cart-item w-commerce-commercecartitem">
      <img src="https://cdn4.wpbeginner.com/wp-content/uploads/2016/11/choose-best-blogging-platform.png" alt="this">
    </div>
  </div>
</section>
<script type="text/x-wf-template" id="wf-template-5b596057-aeee-c8b3-29d3-9001975d39d3">
        <div data-node-type="cart-item" class="cart-item w-commerce-commercecartitem" template-part-bind="5b596057-aeee-c8b3-29d3-9001975d39d4"><img data-node-type="cart-item-image" class="image-19 w-commerce-commercecartitemimage" template-part-bind="5b596057-aeee-c8b3-29d3-9001975d39d5" src="<%= it.image %>">
        <div>
        <a data-node-type="link" class="link-block-10" template-part-bind="1691a2ae-6dd9-ed36-b7ad-3753fe3431fa" href="<%= it.url%>"><div data-node-type="cart-item-info" class="div-block-13 w-commerce-commercecartiteminfo" template-part-bind="5b596057-aeee-c8b3-29d3-9001975d39d6"><div data-node-type="cart-product-name" class="text-block-20 w-commerce-commercecartproductname" template-part-bind="5b596057-aeee-c8b3-29d3-9001975d39d7"><%= it.title %></div><ul class="option-list-3 w-commerce-commercecartoptionlist" template-part-bind="5b596057-aeee-c8b3-29d3-9001975d39d9"><% Object.entries(it.properties).forEach( data =>  { %><li data-node-type="cart-option-list-item"><span data-node-type="cart-option-list-item-label" template-part-bind="5b596057-aeee-c8b3-29d3-9001975d39db"><%= data[0] %></span><span>: </span><span data-node-type="cart-option-list-item-value" template-part-bind="5b596057-aeee-c8b3-29d3-9001975d39de"><%= data[1] %></span></li><% }) %></ul></div></a>
        <input class="w-commerce-commercecartquantity quantity cart" name="<%= it.id %>" type="number" min="0" min="1" value="<%= it.quantity %>" data-node-type="cart-quantity">
      </div>
      <div>
        <div class="div-block-12"><div class="text-block-19" template-part-bind="5b596057-aeee-c8b3-29d3-9001975d39d8"><%= it.price %></div>
      <a data-node-type="cart-remove-link" class="remove-button" href="#" role="button" aria-label="Remove item from cart" template-part-bind="5b596057-aeee-c8b3-29d3-9001975d39df" data-product-id="<%= it.id %>"><div class="text-block-21">Remove</div></a>
    </div>
      <div class="div-block-14"><div class="div-block-11"><ul data-node-type="list" class="list-2"><li data-node-type="list-item"><div class="text-block-18" >Delivery in 2 days , Sun | Free</div></li></ul></div></div>
  </div>
  </div>
</script>
<div
  class="w-commerce-commercecartlist cart-list"
  data-wf-collection="database.commerceOrder.userItems"
  data-wf-template-id="wf-template-5b596057-aeee-c8b3-29d3-9001975d39d3"
></div>
